<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="529.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.csc285.gamebuilder.MainPaneController">
   <center>
<AnchorPane BorderPane.alignment="CENTER">
   <children>
      <Label layoutX="193.0" layoutY="143.0" text="Game builder...">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Button fx:id="uselessButton" layoutX="216.0" layoutY="256.0" mnemonicParsing="false" onAction="#handleUselessButton" text="A Useless Button" />
      <TextField fx:id="uselessTextField" layoutX="207.0" layoutY="200.0" text="(useless text field)" />
   </children>
</AnchorPane>
   </center>
   <top>
<MenuBar BorderPane.alignment="CENTER">
  <menus>
    <Menu mnemonicParsing="false" text="File">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#handleMenuFileClose" text="Close" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Help">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#handleMenuHelpAbout" text="About" />
      </items>
    </Menu>
  </menus>
</MenuBar>
   </top>
</BorderPane>
